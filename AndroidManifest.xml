            android:name="com.example.shafulator.MainActivity"
            android:label="@string/app_name" android:screenOrientation="unspecified" android:icon="@drawable/shuff" android:theme="@android:style/Theme.Holo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>